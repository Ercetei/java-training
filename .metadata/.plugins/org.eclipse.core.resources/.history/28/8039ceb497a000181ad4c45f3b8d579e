 */
public class Pokemon extends Item implements Toy {

    public Pokemon(String packaging) {
		super(packaging);
	}

	public String POKE = "Pokemon";

    public String getName() {
        return POKE;
    }

    public void which() {
    	System.out.println(this.getName());
    }

}