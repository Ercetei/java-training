package com.infotel.tp.tpMeals;

public class Main {
	
	public void Main() {
		MealBuilder mb = new MealBuilder();
		
		Meal meal = 
				mb.addItem(new ChickenBurger())
				.addItem(new Coke())
				.addItem(new Dinosaur())
				.addItem(new Wrapper())
				.build()
				;
		
		System.out.println(meal);
	}
}
