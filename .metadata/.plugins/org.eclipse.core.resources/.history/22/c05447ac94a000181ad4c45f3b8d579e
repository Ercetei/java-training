package com.infotel.tp.tpMeals;

import java.util.*;

/**
 * 
 */
public class Meal {


    /**
     * 
     */
    public List<Item> items = new ArrayList<Item>();
    
    /**
     * Default constructor
     */
    public Meal(MealBuilder builder) {
    	this.items = builder.items;
    }

	public List<Item> getItems() {
		return items;
	}

	public void setItems(List<Item> items) {
		this.items = items;
	}
	
	
}